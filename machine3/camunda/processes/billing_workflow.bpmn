    <bpmn:scriptTask id="Activity_0vstkg8" name="Check prescription validity" scriptFormat="JavaScript">
      <bpmn:incoming>Flow_0ovtgcr</bpmn:incoming>
      <bpmn:outgoing>Flow_0noxz7o</bpmn:outgoing>
      <bpmn:script>var raw = execution.getVariable("pharmacySearch");

// default = false so gateway always has a value
execution.setVariable("prescriptionValid", false);

if (raw == null) {
  execution.setVariable("pharmacyError", "pharmacySearch is null");
} else {
  try {
    var meds = (typeof raw === "string") ? JSON.parse(raw) : raw;
    execution.setVariable("prescriptionValid", Array.isArray(meds) &amp;&amp; meds.length &gt; 0);
  } catch (e) {
    execution.setVariable("pharmacyError", "Parse failed: " + e);
    execution.setVariable("pharmacyRaw", raw);
  }
}</bpmn:script>
    </bpmn:scriptTask>
