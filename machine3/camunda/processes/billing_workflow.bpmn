    <bpmn:scriptTask id="Activity_18nntx4" name="Parse billing result" scriptFormat="javascript">
      <bpmn:incoming>Flow_0fjqm14</bpmn:incoming>
      <bpmn:outgoing>Flow_0ao7qib</bpmn:outgoing>
      <bpmn:script>var raw = execution.getVariable("billingResult");

// defaults so notification never crashes
execution.setVariable("invoiceNumber", "UNKNOWN");
execution.setVariable("invoiceTotal", 0);
execution.setVariable("pdfPath", "");

if (raw &amp;&amp; raw.trim().startsWith("{")) {
  var obj = JSON.parse(raw);
  if (obj.invoice) {
    execution.setVariable("invoiceNumber", obj.invoice.invoiceNumber);
    execution.setVariable("invoiceTotal", obj.invoice.total);
    execution.setVariable("pdfPath", obj.invoice.pdfPath);
  }
}
print("DEBUG totalAmount=" + execution.getVariable("totalAmount"));
print("DEBUG invoiceNumber=" + execution.getVariable("invoiceNumber"));</bpmn:script>
    </bpmn:scriptTask>
