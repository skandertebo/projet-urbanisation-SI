    <bpmn:scriptTask id="Activity_0vk87q0" name="Parse insurance result" scriptFormat="javascript">
      <bpmn:incoming>Flow_02p3hdp</bpmn:incoming>
      <bpmn:outgoing>Flow_1d2m3ij</bpmn:outgoing>
      <bpmn:script>var raw = execution.getVariable("insuranceResult");
var obj = (typeof raw === "string") ? JSON.parse(raw) : raw;

execution.setVariable("insuranceCovered", obj.covered === true);
execution.setVariable("insuranceShare", obj.insuranceShare);
execution.setVariable("patientShare", obj.patientShare);</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:sequenceFlow id="Flow_02p3hdp" sourceRef="Activity_1ds1m7r" targetRef="Activity_0vk87q0" />
