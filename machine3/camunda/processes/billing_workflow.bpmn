    <bpmn:scriptTask id="Activity_02bodt6" name="Prepare billing data" scriptFormat="javascript">
      <bpmn:incoming>Flow_1o200lh</bpmn:incoming>
      <bpmn:outgoing>Flow_1rk1qpb</bpmn:outgoing>
      <bpmn:script>var raw = execution.getVariable("consultations");
var list = (typeof raw === "string") ? JSON.parse(raw) : raw;

if (!list || list.length === 0) {
  throw "No consultations found for patientId=" + execution.getVariable("patientId");
}

var consultation = list[list.length - 1];

execution.setVariable("consultation", consultation);
execution.setVariable("acts", consultation.acts || []);
execution.setVariable("prescription", consultation.prescription || []);
execution.setVariable("patientCin", consultation.patientCin);

var total = 0;
for (var i = 0; i &lt; (consultation.acts || []).length; i++) {
  total += consultation.acts[i].price || 0;
}
execution.setVariable("totalAmount", total);
execution.setVariable("actsJson", JSON.stringify(consultation.acts || []));</bpmn:script>
    </bpmn:scriptTask>
